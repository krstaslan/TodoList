<%- include("header")-%>

<body>
  <div class="box" id="heading">
      <h1> <%=dayName %></h1>
  </div>
  <div class="box">
      <%for(var i=0;i<newItems.length;i++){ %>
        <div class="item">
          <input type="checkbox" >
          <p> <%= newItems[i]%> </p>
        </div>

      <% } %>
      <form class="item" action="/" method="post">
        <input type="text" name="items"placeholder="New Item"autocomplete="off">
        <button type="button" name="button">+</button>
      </form>
  </div>


<%- include("footer")-%>
